   <!DOCTYPE html>
   <html>
     <head>
       <title><%= title %></title>
       <link rel='stylesheet' href='/stylesheets/style.css' />
     </head>
     <body>
       <div id="allPage">
       <%- include('partials/banner') %>
       <%- include('partials/navbar') %>
       <h1><%= title %></h1>
       <main>
        <table>
          <thead>
          <tr>
            <th>Contact Name</th>
            <th>Email</th>
            <th>Contact Number</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<business_contactList.length;i++) {%>
            <tr>
              <td><%= business_contactList[i].name%></td>
              <td><%= business_contactList[i].email%></td>
              <td><%= business_contactList[i].contac_number%></td>
            </tr>
            <% }%>
        </tbody>
        </table>
       </main>
     </div>
     <%- include('partials/footer') %>
     </body>
   </html>